<mxStylesheet>
	<include name="/com/mxgraph/thesis/swing/resources/default-style.xml"/>
	<add as="defaultVertex" extend="defaultVertex">
		<add as="fillColor" value="#add8e6"/>
		<add as="strokeColor" value="black"/>
		<add as="fontSize" value="25"/>
		<add as="fontColor" value="black"/>
		<add as="fontFamily" value="Times"/>
	</add>
	<add as="defaultEdge" extend="defaultEdge">
		<add as="labelBackgroundColor" value="white"/>
		<add as="rounded" value="1"/>
		<add as="strokeColor" value="black"/>
		<add as="fontSize" value="15"/>
		<add as="fontColor" value="#1d258f"/>
		<add as="fontFamily" value="Times"/>
	</add>
	<add as="label">
		<add as="shape" value="label"/>
		<add as="perimeter" value="rectanglePerimeter"/>
		<add as="fontStyle" value="1"/>
		<add as="align" value="left"/>
		<add as="verticalAlign" value="middle"/>
		<add as="spacing" value="2"/>
		<add as="spacingLeft" value="40"/>
		<add as="imageWidth" value="60"/>
		<add as="imageHeight" value="59"/>
		<add as="rounded" value="1"/>
		<add as="shadow" value="1"/>
		<add as="glass" value="1"/>
	</add>
	
	<add as="icon" extend="label">
		<add as="align" value="center"/>
		<add as="imageAlign" value="center"/>
		<add as="verticalLabelPosition" value="bottom"/>
		<add as="verticalAlign" value="top"/>
		<add as="spacingTop" value="4"/>
		<add as="labelBackgroundColor" value="#FFFFFF"/>
		<add as="spacing" value="0"/>
		<add as="spacingLeft" value="0"/>
		<add as="spacingTop" value="6"/>
		<add as="fontStyle" value="0"/>
		<add as="imageWidth" value="48"/>
		<add as="imageHeight" value="48"/>
	</add>
	
	<add as="rectangle">
		<add as="shape" value="rectangle"/>
		<add as="perimeter" value="rectanglePerimeter"/>
		<add as="fillColor" value="#add8e6"/>
	</add>

	<add as="doubleRect">
		<add as="shape" value="doubleRect"/>
		<add as="fillColor" value="#aff0f0"/>
	</add>

	<add as="ellipse">
		<add as="shape" value="ellipse"/>
		<add as="perimeter" value="ellipsePerimeter"/>
		<add as="fillColor" value="#fffacd"/>
	</add>

    <add as="doubleEl" extend= "ellipse">
		<add as="shape" value="doubleEl"/>
        <add as="fillColor" value="#e2e35f"/>
	</add>

	<add as="dashed" extend= "ellipse">
	<add as="shape" value="dashed"/>
		<add as="fillColor" value="#fed8b1"/>
	</add>    


	<add as="partial">
		<add as="shape" value="partial"/>
			<add as="fontStyle" value="6" />
			<add as="fillColor" value="#fffacd"/>
	</add>

	<add as="primary">
		<add as="shape" value="primary"/>
		<add as="fontStyle" value="5" />
			<add as="fillColor" value="#fffacd"/>
	</add>

	<add as="unique">
		<add as="shape" value="unique"/>
			<add as="fontStyle" value="4" />
			<add as="fillColor" value="#fffacd"/>
	</add>

	<add as="un_dl">
		<add as="shape" value="un_dl"/>
		<add as="fontStyle" value="4" />
			<add as="fillColor" value="#fffacd"/>
	</add>

	<add as="rhombus">
		<add as="shape" value="rhombus"/>
		<add as="fillColor" value="#ffb6c1"/>
	</add>		

	<add as="doubleRh" >
		<add as="shape" value="doubleRh"/>
		<add as="fillColor" value="#ffdbe9"/>
	</add>		
		

	<add as="d_circle" extend= "ellipse">
		<add as="shape" value = "d_circle"/>
		<add as="fillColor" value="#64e764"/>
	</add>

	<add as="o_circle" extend= "ellipse">
		<add as="shape" value="o_circle"/>
		<add as="fillColor" value="#82f40b"/>
	</add>

	<add as="U_circle" extend= "ellipse">
		<add as="shape" value="U_circle"/>
		<add as="fillColor" value="#87FDFF"/>
	</add>


	<add as="line">
		<add as="shape" value="line"/>
		<add as="strokeWidth" value="4"/>
		<add as="labelBackgroundColor" value="white"/>
		<add as="verticalAlign" value="top"/>
		<add as="spacingTop" value="8"/>
	</add>

</mxStylesheet>
